<?php

declare(strict_types=1);

namespace Vendor;

class Foo
{
    public function bar() : string
    {
    }

    /**
     * @return resource
     */
    public function baz()
    {
    }

    public function qux()
    {
    }

    public function quux()
    {
        return 'quuz';
    }

    public function quuz() : array
    {
    }

    public function corge() : iterable
    {
    }

    /**
     * @return string[]
     */
    public function grault() : array
    {
    }

    /**
     * @return string[]
     */
    public function garply() : iterable
    {
    }

    public function waldo() : ArrayObject
    {
    }
}

---FIXED---
<?php

declare(strict_types=1);

namespace Vendor;

class Foo
{
    public function bar() : string
    {
    }

    /**
     * @return resource
     */
    public function baz()
    {
    }

    public function qux() : void
    {
    }

    public function quux()
    {
        return 'quuz';
    }

    public function quuz() : array
    {
    }

    public function corge() : iterable
    {
    }

    /**
     * @return array<string>
     */
    public function grault() : array
    {
    }

    /**
     * @return array<string>
     */
    public function garply() : iterable
    {
    }

    public function waldo() : ArrayObject
    {
    }
}

