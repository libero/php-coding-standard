---DESCRIPTION---
Primitive types must be in lower case
---CONTENTS---
<?php

declare(strict_types=1);

/**
 * @param array<string> $qux
 */
function foo(String $bar, ARRAY $baz) : vOiD
{
}

/**
 * @return iterable<string>
 */
function qux(Bool $quux, OBJECT $quuz) : iTeRaBlE
{
}

---FIXED---
<?php

declare(strict_types=1);

/**
 * @param array<string> $qux
 */
function foo(string $bar, array $baz) : void
{
}

/**
 * @return iterable<string>
 */
function qux(bool $quux, object $quuz) : iterable
{
}

---
