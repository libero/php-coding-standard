---FIXED---
<?php

declare(strict_types=1);

namespace Vendor;

interface Foo extends Bar
{
    /**
     * @return resource
     */
    public function getBaz();

    /**
     * @param resource $baz
     */
    public function setBaz($baz) : void;

    public function qux() : void;
}

