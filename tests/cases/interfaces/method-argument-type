---DESCRIPTION---
Method argument types must be declared
---FILENAME---
Foo.php
---CONTENTS---
<?php

declare(strict_types=1);

namespace Vendor;

use ArrayObject;

interface Foo
{
    public function bar($baz) : string;

    /**
     * @param resource $quux
     */
    public function qux($quux) : string;

    public function quuz(array $corge) : string;

    public function grault(iterable $garply) : string;

    public function waldo(ArrayObject $fred) : string;
}

