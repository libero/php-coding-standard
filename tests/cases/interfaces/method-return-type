---DESCRIPTION---
Method return types must be declared
---FILENAME---
Foo.php
---CONTENTS---
<?php

declare(strict_types=1);

namespace Vendor;

interface Foo
{
    public function bar() : string;

    /**
     * @return resource
     */
    public function baz();

    public function qux();
}

---FIXED---
<?php

declare(strict_types=1);

namespace Vendor;

interface Foo
{
    public function bar() : string;

    /**
     * @return resource
     */
    public function baz();

    public function qux() : void;
}

---
