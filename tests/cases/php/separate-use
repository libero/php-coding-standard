---DESCRIPTION---
One use statement per declaration
---CONTENTS---
<?php

namespace Vendor;

use Bar, Foo;
use function baz, qux;
use const QUUX, QUUZ;

new Foo(baz(QUUX));
new Bar(qux(QUUZ));

---FIXED---
<?php

namespace Vendor;

use Bar;
use Foo;
use function baz;
use function qux;
use const QUUX;
use const QUUZ;

new Foo(baz(QUUX));
new Bar(qux(QUUZ));

