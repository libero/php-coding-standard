---DESCRIPTION---
Ternary expressions must not be used unnecessarily
---CONTENTS---
<?php

declare(strict_types=1);

$foo = function () : bool {
    return true ? true : false;
};

$bar = function () : bool {
    return true === false ? false : true;
};

---FIXED---
<?php

declare(strict_types=1);

$foo = function () : bool {
    return true;
};

$bar = function () : bool {
    return true !== false;
};

---
