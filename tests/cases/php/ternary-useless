---DESCRIPTION---
Ternary expressions must not be used unnecessarily
---CONTENTS---
<?php

declare(strict_types=1);

$foo = function ($bar) : bool {
    return $bar ? $bar : false;
};

$baz = function ($qux) : bool {
    return $qux === false ? false : true;
};

---FIXED---
<?php

declare(strict_types=1);

$foo = function ($bar) : bool {
    return $bar ?: false;
};

$baz = function ($qux) : bool {
    return $qux !== false;
};

---
