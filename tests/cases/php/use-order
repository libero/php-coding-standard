---DESCRIPTION---
Use statements must be ordered alphabetically
---CONTENTS---
<?php
namespace Vendor;

use const GARPLY;
use Foo\Barbaz;
use Foo\Barquuz;
use function quuz;
use function Foo\grault;
use Foo\BarQuux;
use const Foo\WALDO;
use Foo\BarQux;
use const Foo\FRED;
use function Foo\corge;

new Barbaz(corge(FRED));
new BarQux(grault(WALDO));
new BarQuux(quuz(GARPLY));
new Barquuz();

---FIXED---
<?php
namespace Vendor;

use Foo\BarQuux;
use Foo\BarQux;
use Foo\Barbaz;
use Foo\Barquuz;
use function Foo\corge;
use function Foo\grault;
use function quuz;
use const Foo\FRED;
use const Foo\WALDO;
use const GARPLY;

new Barbaz(corge(FRED));
new BarQux(grault(WALDO));
new BarQuux(quuz(GARPLY));
new Barquuz();

