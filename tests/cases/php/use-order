---DESCRIPTION---
Use statements must be ordered alphabetically
---CONTENTS---
<?php
namespace Vendor;

use const QUX;
use Foo;
use function quuz;
use const Bar\QUUX;
use Bar\Baz;
use function Bar\corge;

new Foo(quuz(QUX));
new Baz(corge(QUUX));

---FIXED---
<?php
namespace Vendor;

use Bar\Baz;
use Foo;
use function Bar\corge;
use function quuz;
use const Bar\QUUX;
use const QUX;

new Foo(quuz(QUX));
new Baz(corge(QUUX));

---
