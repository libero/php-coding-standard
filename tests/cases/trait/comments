<?php

declare(strict_types=1);

namespace Vendor;

trait Foo
{
    use Bar;

    private $baz;

    /**
     * @return resource
     */
    public function getBaz()
    {
        return $this->baz;
    }

    /**
     * @param resource $baz
     */
    public function setBaz($baz) : void
    {
        $this->baz = $baz;
    }

    public function qux() : void
    {
    }
}

