---DESCRIPTION---
Method braces must be on separate lines.
---FILENAME---
Foo.php
---CONTENTS---
<?php

declare(strict_types=1);

namespace Vendor;

trait Foo
{
    public function bar() : void { $baz = 'qux'; }
}

---FIXED---
<?php

declare(strict_types=1);

namespace Vendor;

trait Foo
{
    public function bar() : void
    {
        $baz = 'qux';
    }
}

---
