---DESCRIPTION---
4 spaces must be used for indenting
---CONTENTS---
<?php

declare(strict_types=1);

function foo() : void
   {
 foreach ([] as $bar) {
echo $bar;
 }
  }

---FIXED---
<?php

declare(strict_types=1);

function foo() : void
{
    foreach ([] as $bar) {
        echo $bar;
    }
}

