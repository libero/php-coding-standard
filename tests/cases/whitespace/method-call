---DESCRIPTION---
Method calls must not have unnecessary whitespace
---CONTENTS---
<?php

declare(strict_types=1);

$foo -> bar ( $arg1 );

$foo
-> bar ($arg1
,$arg2)
-> baz ($arg1
,$arg2);

---FIXED---
<?php

declare(strict_types=1);

$foo->bar($arg1);

$foo->bar(
    $arg1,
    $arg2
)->baz(
    $arg1,
    $arg2
);

---
