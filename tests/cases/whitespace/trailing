<?php 
  
declare(strict_types=1);   
    
function foo() : void     
{      
    foreach ([] as $bar) {       
        echo $bar;        
    }        
}         
          
---FIXED---
<?php

declare(strict_types=1);

function foo() : void
{
    foreach ([] as $bar) {
        echo $bar;
    }
}

