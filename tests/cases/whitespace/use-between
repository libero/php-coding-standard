---DESCRIPTION---
No spaces between use declarations
---CONTENTS---
<?php

namespace Vendor;

use Bar;


use Foo;

use function qux;


use const BAZ;

new Foo(new Bar(qux(BAZ)));

---FIXED---
<?php

namespace Vendor;

use Bar;
use Foo;
use function qux;
use const BAZ;

new Foo(new Bar(qux(BAZ)));

