---DESCRIPTION---
No spaces between use declarations
---CONTENTS---
<?php

namespace Vendor;

use Bar;


use Foo;

use const BAZ;


use function qux;

new Foo(new Bar(qux(BAZ)));

---FIXED---
<?php

namespace Vendor;

use Bar;
use Foo;
use const BAZ;
use function qux;

new Foo(new Bar(qux(BAZ)));

---
